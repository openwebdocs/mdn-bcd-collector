{
  "name": "mdn-bcd-collector",
  "description": "Data collection service for @mdn/browser-compat-data",
  "version": "10.0.5",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "author": "Gooborg Studios (https://www.gooborg.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/GooborgStudios/mdn-bcd-collector.git"
  },
  "engines": {
    "node": ">=18 <20"
  },
  "scripts": {
    "prepare": "ts-node prepare.ts",
    "format": "eslint . && prettier -c \"**/*.json\" \"**/*.md\" \"**/*.scss\" \"**/*.yaml\" --color && ejslint views/*",
    "format:fix": "eslint --fix . && prettier --write \"**/*.json\" \"**/*.md\" \"**/*.scss\" \"**/*.yaml\"",
    "fix": "npm run format:fix",
    "clean": "rm -rf .nyc_output coverage generated tests.json",
    "build": "npm run build:compile && npm run build:tests && npm run build:resources",
    "build:compile": "tsc -p .",
    "build:tests": "ts-node test-builder/index.ts",
    "build:resources": "ts-node prepare-resources.ts",
    "gcp-build": "npm run build",
    "start": "NODE_ENV=production node app.js",
    "dev": "npm i && nodemon -e ts,json,idl,scss,yaml --exec \"npm run build:tests && npm run build:resources && ts-node app.ts\"",
    "unittest": "NODE_ENV=test c8 mocha --recursive './**/*.test.ts'",
    "coverage": "npm run coverage:report && npm run coverage:open",
    "coverage:report": "c8 report -r lcov",
    "coverage:open": "open-cli coverage/lcov-report/index.html",
    "test": "npm run format && tsc -p . --noEmit && npm run unittest",
    "selenium": "ts-node scripts/selenium.ts",
    "update-bcd": "ts-node scripts/update-bcd.ts",
    "find-missing-features": "ts-node scripts/find-missing-features.ts",
    "find-missing-reports": "ts-node scripts/find-missing-reports.ts",
    "add-new-bcd": "ts-node scripts/add-new-bcd.ts",
    "release": "ts-node scripts/release.ts",
    "report-stats": "ts-node scripts/report-stats.ts"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "5.3.0",
    "@google-cloud/storage": "6.11.0",
    "@mdi/font": "7.2.96",
    "@mdn/browser-compat-data": "5.2.67",
    "@octokit/rest": "19.0.13",
    "@webref/elements": "^2.2.1",
    "compare-versions": "6.0.0-rc.1",
    "cookie-parser": "1.4.6",
    "didyoumean": "1.2.2",
    "ejs": "3.1.9",
    "es-main": "1.2.0",
    "express": "4.18.2",
    "express-csp-header": "^5.1.0",
    "express-ejs-layouts": "2.5.1",
    "fs-extra": "11.1.1",
    "highlight.js": "^11.8.0",
    "json-stable-stringify": "1.0.2",
    "marked": "^5.1.0",
    "marked-gfm-heading-id": "^3.0.4",
    "marked-highlight": "^2.0.1",
    "marked-mangle": "^1.0.1",
    "slugify": "1.6.6",
    "ua-parser-js": "1.0.35",
    "unique-string": "3.0.0",
    "wasm-feature-detect": "^1.5.1",
    "winston": "3.9.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/eslint-parser": "7.22.5",
    "@babel/plugin-syntax-import-assertions": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@browser-logos/chrome": "2.0.0",
    "@browser-logos/edge": "2.0.7",
    "@browser-logos/firefox": "3.0.10",
    "@browser-logos/opera": "1.1.11",
    "@browser-logos/safari": "2.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@swc/helpers": "^0.5.0",
    "@types/mocha": "10.0.1",
    "@types/node": "20.3.1",
    "@types/superagent": "^4.1.18",
    "@types/webidl2": "^23.13.6",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@webref/css": "6.5.9",
    "@webref/idl": "3.35.1",
    "c8": "8.0.0",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "chai-fs": "2.0.0",
    "chai-http": "4.4.0",
    "chai-subset": "1.6.0",
    "chalk": "5.2.0",
    "chalk-template": "1.1.0",
    "ejs-lint": "2.0.0",
    "enquirer": "2.3.6",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "fdir": "6.0.1",
    "json3": "3.3.3",
    "klaw": "4.1.0",
    "listr2": "6.6.0",
    "minimatch": "9.0.2",
    "mocha": "10.2.0",
    "node-fetch": "3.3.1",
    "nodemon": "2.0.22",
    "open-cli": "7.2.0",
    "prettier": "2.8.8",
    "puppeteer": "20.7.3",
    "sass": "1.63.6",
    "selenium-webdriver": "4.10.0",
    "sinon": "15.2.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "webidl2": "24.4.0",
    "yaml": "2.3.1"
  }
}
