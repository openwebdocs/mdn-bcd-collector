{
  "name": "mdn-bcd-collector",
  "description": "Data collection service for @mdn/browser-compat-data",
  "version": "10.15.1",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "author": "Gooborg Studios (https://www.gooborg.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/openwebdocs/mdn-bcd-collector.git"
  },
  "engines": {
    "node": "^22",
    "npm": "^11"
  },
  "scripts": {
    "prepare": "tsx prepare.ts",
    "format": "eslint . && prettier -c \"**/*.ts\" \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/*.scss\" \"**/*.yaml\" --color && ejslint views/*",
    "format:fix": "eslint --fix . && prettier --write \"**/*.ts\" \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/*.scss\" \"**/*.yaml\"",
    "fix": "npm run format:fix",
    "clean": "rm -rf .nyc_output coverage generated tests.json",
    "build": "npm run build:compile && npm run build:tests && npm run build:resources",
    "build:compile": "tsc -p .",
    "build:tests": "tsx test-builder/index.ts",
    "build:resources": "tsx prepare-resources.ts",
    "gcp-build": "npm run build",
    "start": "node app.js",
    "dev": "npm i && npm run build:tests && npm run build:resources && nf -j Procfile.dev start",
    "unittest": "NODE_ENV=test c8 mocha --require tsx --recursive './**/*.test.ts'",
    "coverage": "npm run coverage:report && npm run coverage:open",
    "coverage:report": "c8 report -r lcov",
    "coverage:open": "open-cli coverage/lcov-report/index.html",
    "test": "npm run format && npm run test:types && npm run unittest",
    "test:types": "tsc -p . --noEmit",
    "selenium": "tsx scripts/selenium.ts",
    "update-bcd": "tsx scripts/update-bcd.ts",
    "feature-coverage": "tsx scripts/feature-coverage.ts",
    "find-missing-features": "tsx scripts/feature-coverage.ts",
    "find-missing-reports": "tsx scripts/find-missing-reports.ts",
    "add-new-bcd": "tsx scripts/add-new-bcd.ts",
    "release": "tsx scripts/release.ts",
    "report-stats": "tsx scripts/report-stats.ts"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "6.0.1",
    "@google-cloud/storage": "7.17.1",
    "@mdi/font": "7.4.47",
    "@mdn/browser-compat-data": "7.1.10",
    "@octokit/rest": "22.0.0",
    "@swc/core": "1.13.5",
    "compare-versions": "6.1.1",
    "cookie-parser": "1.4.7",
    "didyoumean": "1.2.2",
    "ejs": "3.1.10",
    "es-main": "1.4.0",
    "express": "5.1.0",
    "express-csp-header": "^6.0.0",
    "express-ejs-layouts": "2.5.1",
    "express-session": "^1.18.2",
    "fs-extra": "11.3.2",
    "highlight.js": "^11.11.1",
    "json-stable-stringify": "1.3.0",
    "marked": "^16.3.0",
    "marked-gfm-heading-id": "^4.1.2",
    "marked-mangle": "^1.1.11",
    "mermaid": "^11.12.0",
    "slugify": "1.6.6",
    "tsx": "^4.20.6",
    "ua-parser-js": "2.0.5",
    "wasm-feature-detect": "^1.8.0",
    "winston": "3.18.3",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/eslint-parser": "7.28.4",
    "@babel/plugin-syntax-import-assertions": "7.27.1",
    "@babel/preset-env": "7.28.3",
    "@browser-logos/chrome": "2.0.0",
    "@browser-logos/edge": "2.0.7",
    "@browser-logos/firefox": "3.0.10",
    "@browser-logos/opera": "1.1.11",
    "@browser-logos/safari": "2.1.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.37.0",
    "@listr2/prompt-adapter-enquirer": "^3.0.4",
    "@swc/cli": "^0.7.8",
    "@swc/helpers": "^0.5.17",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "10.0.10",
    "@types/node": "24.7.0",
    "@types/selenium-webdriver": "^4.35.1",
    "@types/superagent": "^8.1.9",
    "@types/webidl2": "^24.4.3",
    "@types/yargs": "^17.0.33",
    "@webref/css": "7.0.11",
    "@webref/elements": "^2.5.1",
    "@webref/idl": "3.68.1",
    "c8": "10.1.3",
    "chai": "4.5.0",
    "chai-as-promised": "8.0.2",
    "chai-fs": "2.0.0",
    "chai-http": "5.1.2",
    "chai-subset": "1.6.0",
    "chalk": "5.6.2",
    "chalk-template": "1.1.2",
    "cheerio": "^1.1.2",
    "core-js": "^3.45.1",
    "css-tree": "^3.1.0",
    "ejs-lint": "^2.0.1",
    "enquirer": "2.4.1",
    "es-scraper": "^0.0.13",
    "eslint": "9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^60.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "^61.0.2",
    "fdir": "6.5.0",
    "foreman": "^3.0.1",
    "globals": "^16.4.0",
    "json3": "3.3.3",
    "jsonc-parser": "^3.3.1",
    "listr2": "9.0.4",
    "minimatch": "10.0.3",
    "mocha": "11.7.4",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.10",
    "open-cli": "8.0.0",
    "prettier": "3.6.2",
    "puppeteer": "24.23.0",
    "sass": "1.93.2",
    "selenium-webdriver": "4.36.0",
    "sinon": "21.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "~8.45.0",
    "webidl2": "24.5.0",
    "yaml": "2.8.1"
  }
}
