{
  "name": "mdn-bcd-collector",
  "description": "Data collection service for @mdn/browser-compat-data",
  "version": "10.6.4",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "author": "Gooborg Studios (https://www.gooborg.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/openwebdocs/mdn-bcd-collector.git"
  },
  "engines": {
    "node": ">=18 <21",
    "npm": ">=9"
  },
  "scripts": {
    "prepare": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning prepare.ts",
    "format": "eslint . && prettier -c \"**/*.ts\" \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/*.scss\" \"**/*.yaml\" --color && ejslint views/*",
    "format:fix": "eslint --fix . && prettier --write \"**/*.ts\" \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/*.scss\" \"**/*.yaml\"",
    "fix": "npm run format:fix",
    "clean": "rm -rf .nyc_output coverage generated tests.json",
    "build": "npm run build:compile && npm run build:tests && npm run build:resources",
    "build:compile": "tsc -p .",
    "build:tests": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning test-builder/index.ts",
    "build:resources": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning prepare-resources.ts",
    "gcp-build": "npm run build",
    "start": "node app.js",
    "dev": "npm i && npm run build:tests && npm run build:resources && nf -j Procfile.dev start",
    "unittest": "NODE_ENV=test c8 mocha --recursive './**/*.test.ts'",
    "coverage": "npm run coverage:report && npm run coverage:open",
    "coverage:report": "c8 report -r lcov",
    "coverage:open": "open-cli coverage/lcov-report/index.html",
    "test": "npm run format && npm run test:types && npm run unittest",
    "test:types": "tsc -p . --noEmit",
    "selenium": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning scripts/selenium.ts",
    "update-bcd": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning scripts/update-bcd.ts",
    "find-missing-features": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning scripts/find-missing-features.ts",
    "find-missing-reports": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning scripts/find-missing-reports.ts",
    "add-new-bcd": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning scripts/add-new-bcd.ts",
    "release": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning scripts/release.ts",
    "report-stats": "node --loader=ts-node/esm --no-warnings=ExperimentalWarning scripts/report-stats.ts"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "6.0.0",
    "@google-cloud/storage": "7.7.0",
    "@mdi/font": "7.3.67",
    "@mdn/browser-compat-data": "5.5.1",
    "@octokit/rest": "20.0.2",
    "@swc/core": "1.3.100",
    "@webref/elements": "^2.2.2",
    "compare-versions": "6.1.0",
    "cookie-parser": "1.4.6",
    "didyoumean": "1.2.2",
    "ejs": "3.1.9",
    "es-main": "1.3.0",
    "express": "4.18.2",
    "express-csp-header": "^5.2.1",
    "express-ejs-layouts": "2.5.1",
    "express-session": "^1.17.3",
    "fs-extra": "11.2.0",
    "highlight.js": "^11.9.0",
    "json-stable-stringify": "1.1.0",
    "marked": "^11.1.0",
    "marked-gfm-heading-id": "^3.1.2",
    "marked-highlight": "^2.1.0",
    "marked-mangle": "^1.1.6",
    "slugify": "1.6.6",
    "ts-node": "10.9.2",
    "ua-parser-js": "1.0.37",
    "wasm-feature-detect": "^1.6.1",
    "winston": "3.11.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/eslint-parser": "7.23.3",
    "@babel/plugin-syntax-import-assertions": "7.23.3",
    "@babel/preset-env": "7.23.6",
    "@browser-logos/chrome": "2.0.0",
    "@browser-logos/edge": "2.0.7",
    "@browser-logos/firefox": "3.0.10",
    "@browser-logos/opera": "1.1.11",
    "@browser-logos/safari": "2.1.0",
    "@listr2/prompt-adapter-enquirer": "^2.0.0",
    "@swc/cli": "^0.1.63",
    "@swc/helpers": "^0.5.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.5",
    "@types/selenium-webdriver": "^4.1.21",
    "@types/superagent": "^4.1.24",
    "@types/webidl2": "^24.4.3",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@webref/css": "6.10.0",
    "@webref/idl": "3.42.1",
    "c8": "8.0.1",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "chai-fs": "2.0.0",
    "chai-http": "4.4.0",
    "chai-subset": "1.6.0",
    "chalk": "5.3.0",
    "chalk-template": "1.1.0",
    "cheerio": "^1.0.0-rc.12",
    "core-js": "^3.34.0",
    "ejs-lint": "2.0.0",
    "enquirer": "2.4.1",
    "es-scraper": "^0.0.7",
    "eslint": "8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^46.9.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "^49.0.0",
    "fdir": "6.1.1",
    "foreman": "^3.0.1",
    "json3": "3.3.3",
    "listr2": "8.0.0",
    "minimatch": "9.0.3",
    "mocha": "10.2.0",
    "node-fetch": "3.3.2",
    "nodemon": "3.0.2",
    "open-cli": "8.0.0",
    "prettier": "3.1.1",
    "puppeteer": "21.6.1",
    "sass": "1.69.5",
    "selenium-webdriver": "4.16.0",
    "sinon": "17.0.1",
    "typescript": "5.3.3",
    "webidl2": "24.4.1",
    "yaml": "2.3.4"
  }
}
